<h1>Welcome to SmartGroup!</h1>

<h2 class="subheader">Upload a Course</h2>


    <a href="#" class="button success">Upload Course</a>




<h2 class="subheader">Current Courses</h2>

<table id="course_list">
  <% @courses.each do |course| %>
    <tr>
      <td ><b><%= course.name %></b></td>
      <td><a href="#" class="small button" id="opt_bttn">Group Generator</a></td>
      <td><a href="#" class="small button" id="opt_bttn">Metics / Graphs</a></td>
      <td><a href="#" class="small button" id="opt_bttn">Edit Roster</a></td>
    </tr>
  <% end %>
</table>
